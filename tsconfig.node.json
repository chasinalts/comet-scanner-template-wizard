{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "types": ["node"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "target": "ESNext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": false
  },
  "include": ["vite.config.ts"]
}
